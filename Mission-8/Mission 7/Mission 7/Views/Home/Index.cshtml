@{
    ViewData["Title"] = "Home Page";
}

@model BooksViewModel

<h1 class="mb-4 ">Book Store</h1>

@foreach (Book b in Model.Books)
{
    <partial name="BookSummary" model="b"/>
}

<div page-blah="@Model.PageInfo" page-action="Index" page-class="btn" page-class-enabled="true"
     page-class-normal="btn-outline-dark" page-class-selected ="btn-primary" class="btn-group pull-right m-1"> </div>

